{
  "name": "reconnect-api",
  "version": "1.0.0",
  "type": "module",
  "main": "server.ts",
  "scripts": {
    "init": "sh ./src/scripts/init.sh",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "db:local": "prisma dev --name webhook-dev",
    "db:local:clean": "prisma dev rm webhook-dev",
    "db:studio": "prisma studio",
    "db:migrate:new": "prisma migrate dev",
    "dev": "tsx watch --inspect src/server.ts",
    "dev:server": "tsx watch --inspect src/server.ts",
    "build": "tsc",
    "start": "tsx src/server.js",
    "db:testdb": "prisma dev --name testdb",
    "db:generate": "prisma generate",
    "db:migratedev": "prisma migrate dev",
    "db:migrate": "prisma migrate deploy",
    "db:push": "prisma db push --skip-generate",
    "db:reset": "prisma migrate reset",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@electric-sql/pglite": "^0.3.4",
